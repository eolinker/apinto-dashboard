{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "apinto": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "eo-ng",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "deployUrl": "/frontend/",
            "outputPath": "./dist",
            "index": "projects/core/src/index.html",
            "polyfills": "projects/core/src/polyfills.ts",
            "main": "projects/core/src/main.ts",
            "tsConfig": "projects/core/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/core/src/favicon.ico",
              "projects/core/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/ace-builds/src-noconflict/",
                "output": "/ace-builds/"
              }
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "node_modules/eo-ng-layout/styles.scss",
              "node_modules/eo-ng-breadcrumb/styles.scss",
              "node_modules/eo-ng-button/styles.scss",
              "node_modules/eo-ng-cascader/styles.scss",
              "node_modules/eo-ng-checkbox/styles.scss",
              "node_modules/eo-ng-codebox/lib/codebox.component.scss",
              "node_modules/eo-ng-collapse/lib/eo-ng-collapse.component.scss",
              "node_modules/eo-ng-date-picker/styles.scss",
              "node_modules/eo-ng-empty/styles.scss",
              "node_modules/eo-ng-feedback/styles.scss",
              "node_modules/eo-ng-input/styles.scss",
              "node_modules/eo-ng-menu/styles.scss",
              "node_modules/eo-ng-radio/styles.scss",
              "node_modules/eo-ng-select/styles.scss",
              "node_modules/eo-ng-switch/styles.scss",
              "node_modules/eo-ng-table/styles.scss",
              "node_modules/eo-ng-tabs/styles.scss",
              "node_modules/eo-ng-tree/styles.scss",
              "projects/core/src/styles.scss",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css",
              "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.css",
              "node_modules/prismjs/plugins/command-line/prism-command-line.css",
              "node_modules/katex/dist/katex.min.css",
              "node_modules/github-markdown-css/github-markdown.css"
            ],
            "scripts": ["node_modules/clipboard/dist/clipboard.min.js"],
            "allowedCommonJsDependencies": [
              "ace-builds",
              "ace-builds/src-noconflict/ext-language_tools"
            ],
            "customWebpackConfig": {
              "path": "projects/core/webpack.config.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "customWebpackConfig": {
                "path": "projects/core/webpack.config.ts"
              },
              "fileReplacements": [
                {
                  "replace": "projects/core/src/environments/environment.ts",
                  "with": "projects/core/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "business": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/core/src/environments/environment.ts",
                  "with": "projects/core/src/environments/environment.business.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "deployUrl": ""
            },
            "businessDevelopment": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "projects/core/src/environments/environment.ts",
                  "with": "projects/core/src/environments/environment.business.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "apinto:build:production"
            },
            "business": {
              "browserTarget": "apinto:build:business"
            },
            "development": {
              "browserTarget": "apinto:build:development"
            },
            "businessDevelopment": {
              "browserTarget": "apinto:build:businessDevelopment"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-builders/custom-webpack:extract-i18n",
          "options": {
            "browserTarget": "apinto:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "projects/eo-ng-apinto-table/src/test.ts",
            "tsConfig": "projects/eo-ng-apinto-table/tsconfig.spec.json",
            "karmaConfig": "projects/eo-ng-apinto-table/karma.conf.js"
          }
        }
      }
    },
    "eoNgApintoTable": {
      "projectType": "library",
      "root": "projects/eo-ng-apinto-table",
      "sourceRoot": "projects/eo-ng-apinto-table/src",
      "prefix": "eo-ng",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:ng-packagr",
          "options": {
            "project": "projects/eo-ng-apinto-table/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/eo-ng-apinto-table/tsconfig.lib.prod.json"
            },
            "business": {
              "tsConfig": "projects/eo-ng-apinto-table/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/eo-ng-apinto-table/tsconfig.lib.json"
            },
            "businessDevelopment": {
              "tsConfig": "projects/eo-ng-apinto-table/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "projects/eo-ng-apinto-table/src/test.ts",
            "tsConfig": "projects/eo-ng-apinto-table/tsconfig.spec.json",
            "karmaConfig": "projects/eo-ng-apinto-table/karma.conf.js"
          }
        }
      }
    },
    "apintoAuth": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/apinto-auth",
      "sourceRoot": "projects/apinto-auth/src",
      "prefix": "auth",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "deployUrl": "/plugin-frontend/auth/",
            "outputPath": "dist/apinto-auth",
            "index": "projects/apinto-auth/src/index.html",
            "main": "projects/apinto-auth/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/apinto-auth/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/apinto-auth/src/favicon.ico",
              "projects/apinto-auth/src/assets"
            ],
            "styles": ["projects/apinto-auth/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "projects/apinto-auth/webpack.config.ts"
            },
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/core/src/environments/environment.ts",
                  "with": "projects/core/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "deployUrl": ""
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "apintoAuth:build:production"
            },
            "development": {
              "browserTarget": "apintoAuth:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 3333,
            "publicHost": "http://localhost:3333"
          }
        },
        "extract-i18n": {
          "builder": "@angular-builders/custom-webpack:extract-i18n",
          "options": {
            "browserTarget": "apintoAuth:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/apinto-auth/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/apinto-auth/src/favicon.ico",
              "projects/apinto-auth/src/assets"
            ],
            "styles": ["projects/apinto-auth/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "apinto",
  "cli": {
    "analytics": false
  }
}
