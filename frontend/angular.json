{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "appRoot": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/app-root",
      "sourceRoot": "projects/app-root/src",
      "prefix": "app-root",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-root",
            "index": "projects/app-root/src/index.html",
            "main": "projects/app-root/src/main.ts",
            "polyfills": "projects/app-root/src/polyfills.ts",
            "tsConfig": "projects/app-root/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-root/src/favicon.ico",
              "projects/app-root/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/ace-builds/src-noconflict/",
                "output": "/ace-builds/"
              }
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "projects/app-root/src/styles.scss",
              "projects/ams/src/styles.scss",
              "projects/aat/src/styles.scss",
              "node_modules/@toast-ui/editor/dist/toastui-editor.css",
              "node_modules/@toast-ui/editor/dist/theme/toastui-editor-dark.css",
              "node_modules/prismjs/themes/prism.css",
              "node_modules/@toast-ui/editor-plugin-code-syntax-highlight/dist/toastui-editor-plugin-code-syntax-highlight.css"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@toast-ui/editor-plugin-code-syntax-highlight/dist/toastui-editor-plugin-code-syntax-highlight-all.js",
              "@toast-ui/editor-plugin-color-syntax",
              "@toast-ui/editor-plugin-table-merged-cell",
              "ace-builds",
              "ace-builds/src-noconflict/ext-language_tools",
              "crypto-js",
              "fast-xml-parser",
              "mockjs",
              "comment-json",
              "@toast-ui/editor/dist/i18n/zh-cn",
              "@toast-ui/editor-plugin-code-syntax-highlight"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "10mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/app-root/src/environments/environment.ts",
                  "with": "projects/app-root/src/environments/environment.prod.ts"
                }
              ],

              "aot": false,
              "buildOptimizer": false,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              },
              "outputHashing": "all"
            },
            "vspex": {
              "aot": false,
              "buildOptimizer": false,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              },
              "fileReplacements": [
                {
                  "replace": "projects/app-root/src/environments/environment.ts",
                  "with": "projects/app-root/src/environments/environment.vspex.dev.ts"
                }
              ]
            },
            "test": {
              "aot": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "projects/app-root/src/environments/environment.ts",
                  "with": "projects/app-root/src/environments/environment.test.ts"
                }
              ]
            },
            "preview": {
              "aot": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "projects/app-root/src/environments/environment.ts",
                  "with": "projects/app-root/src/environments/environment.pre.ts"
                }
              ]
            },
            "vspexDevelopment": {
              "aot": false,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "outputHashing": "none",
              "subresourceIntegrity": false,
              "fileReplacements": [
                {
                  "replace": "projects/app-root/src/environments/environment.ts",
                  "with": "projects/app-root/src/environments/environment.vspex.dev.ts"
                }
              ]
            },
            "vspexTest": {
              "aot": false,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": false,
              "extractLicenses": false,
              "sourceMap": false,
              "namedChunks": true,
              "outputHashing": "none",
              "subresourceIntegrity": false,
              "fileReplacements": [
                {
                  "replace": "projects/app-root/src/environments/environment.ts",
                  "with": "projects/app-root/src/environments/environment.vspex.test.ts"
                }
              ]
            },
            "development": {
              "aot": false,
              "buildOptimizer": false,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              }
            },
            "saasLocalhostDevelopment": {
              "aot": false,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "outputHashing": "none",
              "subresourceIntegrity": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "appRoot:build:production"
            },
            "development": {
              "browserTarget": "appRoot:build:development"
            },
            "vspex": {
              "browserTarget": "appRoot:build:vspex"
            },
            "vspexTest": {
              "browserTarget": "appRoot:build:vspexTest"
            },
            "vspexDevelopment": {
              "browserTarget": "appRoot:build:vspexDevelopment"
            },
            "saasLocalhostDevelopment": {
              "browserTarget": "appRoot:build:saasLocalhostDevelopment"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "appRoot:build"
          }
        }
      }
    },
