{
  "name": "apinto",
  "version": "3.2.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve --proxy-config proxy.config.json ",
    "start:auth": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve apintoAuth --proxy-config proxy.config.json ",
    "start:demo": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve  --proxy-config proxy.config.demo.json ",
    "start:business": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve  --configuration=businessDevelopment --proxy-config proxy.config.json ",
    "build": "set NODE_OPTIONS=--max_old_space_size=8192 & ng build --configuration production --aot",
    "build:auth": "set NODE_OPTIONS=--max_old_space_size=8192 & ng build apintoAuth --configuration production --aot",
    "build:apintopro": "set NODE_OPTIONS=--max_old_space_size=8192 & ng build --configuration=business  --aot",
    "build:business": "npm-run-all build:apintopro build:auth ",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "eslint": "eslint --fix projects --ext .ts",
    "e2e": "nyc --reporter=lcov --reporter=html --report-dir=./e2e_coverage jest -c jest.e2e.config.ts --detectOpenHandles --forceExit"
  },
